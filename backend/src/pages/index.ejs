<%- include('header'); -%>

<main>
	<div class="container">
		<% share.forEach(data => { %>
		<ul class="share">
			<li>
				<h1><%= data.symbol %></h1>
			</li>
			<li>
				<h1><%= data.open %></h1>
			</li>
			<li>
				<h1><%= data.high %></h1>
			</li>
			<li>
				<h1><%= data.low %></h1>
			</li>

			<li>
				<a href="/<%= data.id %>">View Share &#x21DB;</a>
			</li>
		</ul>
		<% }) %>
	</div>
</main>
<%- include('footer'); -%>
